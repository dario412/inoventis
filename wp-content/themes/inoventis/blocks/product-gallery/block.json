{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "inoventis/product-gallery",
	"title": "Product Gallery",
	"category": "inoventis-product",
	"icon": "images-alt2",
	"description": "Add a gallery of product images.",
	"keywords": [ "product", "gallery", "images" ],
	"textdomain": "inoventis",
	"editorStyle": "file:./editor.css",
	"attributes": {
		"galleryIds": {
			"type": "array",
			"default": []
		}
	},
	"usesContext": [ "postId", "postType" ],
	"supports": {
		"html": false,
		"align": false
	}
}

