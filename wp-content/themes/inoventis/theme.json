{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "840px",
			"wideSize": "1200px"
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"link": true,
			"palette": [
				{
					"slug": "primary",
					"color": "#0073aa",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#005177",
					"name": "Secondary"
				},
				{
					"slug": "accent",
					"color": "#00a0d2",
					"name": "Accent"
				},
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#1e1e1e",
					"name": "Foreground"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "gray-light",
					"color": "#f5f5f5",
					"name": "Gray Light"
				},
				{
					"slug": "gray-dark",
					"color": "#666666",
					"name": "Gray Dark"
				}
			],
			"gradients": [
				{
					"slug": "primary-gradient",
					"gradient": "linear-gradient(135deg, #0073aa 0%, #00a0d2 100%)",
					"name": "Primary Gradient"
				},
				{
					"slug": "secondary-gradient",
					"gradient": "linear-gradient(135deg, #005177 0%, #0073aa 100%)",
					"name": "Secondary Gradient"
				}
			]
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"slug": "system",
					"name": "System Font"
				},
				{
					"fontFamily": "Georgia, serif",
					"slug": "serif",
					"name": "Serif"
				},
				{
					"fontFamily": "'Courier New', Courier, monospace",
					"slug": "monospace",
					"name": "Monospace"
				}
			],
			"fontSizes": [
				{
					"slug": "xs",
					"size": "0.75rem",
					"name": "Extra Small"
				},
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.25rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "1.5rem",
					"name": "Extra Large"
				},
				{
					"slug": "xx-large",
					"size": "2rem",
					"name": "2X Large"
				},
				{
					"slug": "xxx-large",
					"size": "2.5rem",
					"name": "3X Large"
				},
				{
					"slug": "huge",
					"size": "3rem",
					"name": "Huge"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textDecoration": true,
			"textTransform": true
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"operator": "*",
				"increment": 1.5,
				"steps": 7,
				"mediumStep": 1.5,
				"unit": "rem"
			},
			"spacingSizes": [
				{
					"slug": "xxs",
					"size": "0.25rem",
					"name": "2X Extra Small"
				},
				{
					"slug": "xs",
					"size": "0.5rem",
					"name": "Extra Small"
				},
				{
					"slug": "small",
					"size": "0.75rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.5rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "2rem",
					"name": "Extra Large"
				},
				{
					"slug": "xx-large",
					"size": "3rem",
					"name": "2X Large"
				},
				{
					"slug": "xxx-large",
					"size": "4rem",
					"name": "3X Large"
				}
			],
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"shadow": {
			"presets": [
				{
					"slug": "small",
					"name": "Small",
					"shadow": "0 1px 3px rgba(0, 0, 0, 0.12), 0 1px 2px rgba(0, 0, 0, 0.24)"
				},
				{
					"slug": "medium",
					"name": "Medium",
					"shadow": "0 3px 6px rgba(0, 0, 0, 0.16), 0 3px 6px rgba(0, 0, 0, 0.23)"
				},
				{
					"slug": "large",
					"name": "Large",
					"shadow": "0 10px 20px rgba(0, 0, 0, 0.19), 0 6px 6px rgba(0, 0, 0, 0.23)"
				}
			]
		},
		"custom": {
			"spacing": {
				"small": "1rem",
				"medium": "1.5rem",
				"large": "2rem"
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"spacing": {
			"padding": {
				"top": "var(--wp--preset--spacing--medium)",
				"bottom": "var(--wp--preset--spacing--medium)"
			},
			"blockGap": "var(--wp--preset--spacing--large)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "700",
					"lineHeight": "1.2",
					"fontFamily": "var(--wp--preset--font-family--system)"
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)",
					"fontWeight": "700",
					"lineHeight": "1.3",
					"fontFamily": "var(--wp--preset--font-family--system)"
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "600",
					"lineHeight": "1.4",
					"fontFamily": "var(--wp--preset--font-family--system)"
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600",
					"lineHeight": "1.4",
					"fontFamily": "var(--wp--preset--font-family--system)"
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600",
					"lineHeight": "1.5",
					"fontFamily": "var(--wp--preset--font-family--system)"
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--small)"
					}
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600",
					"lineHeight": "1.5",
					"fontFamily": "var(--wp--preset--font-family--system)"
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--small)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent)"
				},
				"typography": {
					"textDecoration": "underline"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600",
					"fontFamily": "var(--wp--preset--font-family--system)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--large)",
						"right": "var(--wp--preset--spacing--large)"
					}
				},
				"border": {
					"radius": "4px",
					"width": "0px"
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--large)",
						"right": "var(--wp--preset--spacing--large)"
					}
				},
				"border": {
					"radius": "4px"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}

